{
  "name": "yusui-tools",
  "upperName": "YusuiTools",
  "version": "1.7.0",
  "private": true,
  "description": "Collection of development tools",
  "author": "SoulLyoko<https://github.com/SoulLyoko>",
  "license": "MIT",
  "homepage": "https://github.com/SoulLyoko/yusui-tools#readme",
  "repository": "https://github.com/SoulLyoko/yusui-tools.git",
  "scripts": {
    "build": "esno scripts/build.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "ci": "pnpm typecheck && pnpm lint && pnpm test run && pnpm build && pnpm docs:build",
    "copy:utils": "esno scripts/copy-utils.ts",
    "docs:build": "vitepress build packages",
    "docs:dev": "vitepress dev packages --host",
    "docs:serve": "vitepress serve packages",
    "lint": "eslint --ext .ts,.vue .",
    "prepare": "husky install",
    "release": "bump && esno scripts/release.ts",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@dcloudio/types": "^3.0.20",
    "@dcloudio/uni-ui": "^1.4.23",
    "@iconify/vue": "4.0.0",
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@smallwei/avue": "^3.2.11",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.191",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.17",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.9.0",
    "@yusui/eslint-config": "workspace:*",
    "@yusui/prettier-config": "workspace:*",
    "@yusui/plugins": "workspace:*",
    "axios": "^0.21.4",
    "c8": "^7.12.0",
    "conventional-changelog-cli": "^2.2.2",
    "element-plus": "^2.2.27",
    "eslint": "^8.30.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-sass": "^5.1.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3",
    "prettier": "^2.8.1",
    "sass": "1.57.1",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.4",
    "uview-plus": "^3.1.20",
    "vite": "^4.0.3",
    "vitepress": "1.0.0-alpha.33",
    "vitest": "^0.26.2",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.17"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
