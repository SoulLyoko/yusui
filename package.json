{
  "name": "yusui-tools",
  "upperName": "YusuiTools",
  "version": "1.3.7",
  "private": true,
  "description": "Collection of development tools",
  "author": "SoulLyoko<https://github.com/SoulLyoko>",
  "license": "MIT",
  "homepage": "https://github.com/SoulLyoko/yusui-tools#readme",
  "repository": "https://github.com/SoulLyoko/yusui-tools.git",
  "scripts": {
    "build": "vite build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "ci": "pnpm typecheck && pnpm lint && pnpm test run && pnpm build && pnpm docs:build",
    "docs:build": "vitepress build packages",
    "docs:dev": "vitepress dev packages",
    "docs:serve": "vitepress serve packages",
    "lint": "eslint --ext .ts,.vue .",
    "prepare": "husky install",
    "postbuild": "esno scripts/postbuild.ts",
    "release": "bump && esno scripts/release.ts",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@iconify/vue": "4.0.0",
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@smallwei/avue": "^3.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.186",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.8.4",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.3.0",
    "@yusui/eslint-config": "workspace:*",
    "@yusui/prettier-config": "workspace:*",
    "c8": "^7.12.0",
    "conventional-changelog-cli": "^2.2.2",
    "element-plus": "^2.2.17",
    "eslint": "^8.25.0",
    "esno": "^0.16.3",
    "fs-extra": "^10.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-sass": "^5.1.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.1",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.2",
    "prettier": "^2.7.1",
    "sass": "1.55.0",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.7",
    "vitepress": "1.0.0-alpha.19",
    "vitest": "^0.24.1",
    "vue": "^3.2.37",
    "vue-tsc": "^1.0.5"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
